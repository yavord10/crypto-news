 getImg = () => {
        this.finalUrls = [];
        this.database.limitToLast(1).on('value', snapshot => {
            snapshot.forEach(childSnapshot => {
                let childVal = childSnapshot.val();
                this.setState({firstNews:childVal});
            });
            this.urlArray.forEach((item) => {
                if (item.includes(this.state.firstNews.img[0])) {
                    this.setState({firstUrl: item});
                    this.finalUrls.unshift(this.state.firstUrl);
                    console.log(this.finalUrls[0])
                    this.setState({finalUrls:this.finalUrls});
                }
            });
        });
        this.imgNames = [];
        let arrNames = [];
        let value = this.context;
        value.topNews.forEach((item) => {
            let imgName = item.img ? item.img[0].toString().toLowerCase() : null;
            arrNames.push(imgName);
            this.imgNames = arrNames.filter(imgName => imgName !== null)
            this.imgNames.shift();
        })
        this.imgNames.forEach((imgName) => {
            for (let item in this.urlArray) {
                if (this.urlArray[item].includes(imgName)) {
                    this.finalUrls.push(this.urlArray[item])
                    this.setState({finalUrls:this.finalUrls});
                } 
            }
        })
        console.log(this.state.finalUrls);
        console.log(this.refNames);
        console.log('executed');
    };